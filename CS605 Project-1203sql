Drop Table HologicTrialLine_T         Cascade Constraints;

create table HologicTrialLine_T 
(ClinicalTrialID number(4,0) not null,
PartnerID number(4,0) not null,
Status varchar(10) not null,
constraint HologicTrialLine_PK Primary key (ClinicalTrialID,PartnerID),
Constraint HologicTrialLine_FK1 foreign key (ClinicalTrialID) references HologicClinicalTrial_T(ClinicalTrialID),
Constraint HologicTrialLine_FK2 foreign key (PartnerID) references HologicPartner_T(PartnerID)
);

