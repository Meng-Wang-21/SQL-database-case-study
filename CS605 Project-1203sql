ProductDescription    VARCHAR(100),
ProductUnitCost     DECIMAL(8,2),
ProductStandardPrice     DECIMAL(8,2),
OnMarketDate    DATE,
CONSTRAINT Product_PK PRIMARY KEY(ProductID)); 

