drop table HologicClinicalTrial_T cascade constraints;

create table HologicClinicalTrial_T
(ClinicalTrialID number(4,0) not null,
TrialName varchar(100) not null,
StartDate date,
ResearcherID number(4,0) not null,
ProductID number(10,0) not null,
TrialResult varchar(10) not null,
constraint HologicClinicalTrial_PK Primary key (ClinicalTrialID),
Constraint HologicClinicalTrial_FK1 foreign key (ResearcherID) references HologicRD_T(ResearcherID),
Constraint HologicClinicalTrial_FK2 foreign key (ProductID) references HologicProduct_T(ProductID)
);

