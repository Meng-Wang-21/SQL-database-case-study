drop table HologicOrder_T cascade constraints;

CREATE TABLE HologicOrder_T
(OrderID                NUMBER(10,0)  NOT NULL,
 MedicalCenterID        CHAR(6)       NOT NULL,
 OrderDate              Date,
 OrderTotalPrice        DECIMAL(8,2),
 OrderDiscount          DECIMAL(8,2),
 OrderFinalPrice        DECIMAL(8,2),
 SalesPersonID          NUMBER(10,0)  NOT NULL,
CONSTRAINT HologicOrder_PK PRIMARY KEY (OrderID),
CONSTRAINT HologicOrder_FK1 FOREIGN KEY (MedicalCenterID) REFERENCES HologicMedicalCenter_T(MedicalCenterID),
CONSTRAINT HologicOrder_FK2 FOREIGN KEY (SalesPersonID) REFERENCES HologicSalesPerson_T(SalesPersonID));


