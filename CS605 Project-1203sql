CONSTRAINT HologicOrderLine_FK1 FOREIGN KEY (OrderID) REFERENCES HologicOrder_T(OrderID),
CONSTRAINT HologicOrderLine_FK2 FOREIGN KEY (ProductID) REFERENCES HologicProduct_T(ProductID));

