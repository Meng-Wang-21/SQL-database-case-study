Drop Table HologicShipment_T         Cascade Constraints;

Create Table HologicShipment_T
                     (ShipmentID      VARCHAR(10)                  not null,
                      CarrierName     Varchar(10),
    
                      ShipDate        Date,
                      Weight          Number(25,0),
                      OrderID         Number(10,0)                 not null,
                      ProductID       Number(10,0)                 not null,
Constraint HologicShipmentT_PK Primary Key (ShipmentID),
Constraint HologicShipmentT_FK1 Foreign Key (OrderID) References HologicOrder_T (OrderID),
Constraint HologicShipmentT_FK2 Foreign Key (ProductID) References HologicProduct_T (ProductID));


